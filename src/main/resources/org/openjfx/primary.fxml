<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.PrimaryViewModel">
    <children>
        <Label text="What do you want me to say?"/>
        <Label fx:id="sayItLabel" text="(currently nothing obviously)"/>
        <TextField fx:id="inputTextfield"/>
        <Button fx:id="sayItButton" mnemonicParsing="false" onAction="#onSubmitClicked" text="Say it!"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
